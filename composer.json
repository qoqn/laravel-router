{
  "name": "poshtive/router",
  "type": "laravel-package",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Poshtive\\Router\\": "src/"
    }
  },
  "require": {
    "php": "^8.3",
    "illuminate/contracts": "^12.0",
    "illuminate/support": "^12.0",
    "illuminate/routing": "^12.0",
    "illuminate/pipeline": "^12.0",
    "symfony/finder": "^7.0"
  },
  "require-dev": {},
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Poshtive\\Router\\RouterServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
